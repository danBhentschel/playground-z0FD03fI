Command[0]="lsblk"
Comment[0]="sda1 is mounted on /"
Command[1]="df -h ."
Comment[1]="/dev/sda1 is 93% full"
Command[2]="ls /dev"
Comment[2]="Do you see sda1 in the list?"
Command[3]="dd if=/dev/zero of=junk bs=1M count=50"
Comment[3]="File created"
Command[4]="df -h ."
Comment[4]="/dev/sda1 is now 96% full"
Command[5]="rm junk"
Comment[5]="File gone"
Command[6]="df -h ."
Comment[6]="/dev/sda1 is 93% full again"
